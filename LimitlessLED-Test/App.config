<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
    </startup>
  <appSettings>
    <!-- use bridge IP address or subnet broadast address 255-->
    <add key="ip" value="192.168.1.255" />
     <!-- use port 8899 for API v3 and port 50000 for v2 bridges -->
    <add key="port" value="8899" />
  </appSettings>
</configuration>